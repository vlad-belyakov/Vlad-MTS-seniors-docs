{
  "$schema": "https://json-schema.org/draft/2019-09/schema#",
  "description": "Модель смены пароля.",
  "required": [
    "newPassword",
    "oldPassword"
  ],
  "properties": {
    "oldPassword": {
      "description": "Старый пароль.",
      "type": "string",
      "example": "12324215Qew"
    },
    "newPassword": {
      "description": "Новый пароль.",
      "type": "string",
      "example": "12312dsafHGH"
    }
  }
}